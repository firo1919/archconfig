      "pacman-key --init",
      "pacman-key --populate archlinux",
      "pacman-key --recv-key 3056513887B78AEB --keyserver keyserver.ubuntu.com",
      "pacman-key --lsign-key 3056513887B78AEB",
      "pacman -U 'https://cdn-mirror.chaotic.cx/chaotic-aur/chaotic-keyring.pkg.tar.zst'",
      "pacman -U 'https://cdn-mirror.chaotic.cx/chaotic-aur/chaotic-mirrorlist.pkg.tar.zst'"
      "echo -e '\n[chaotic-aur]\nInclude = /etc/pacman.d/chaotic-mirrorlist\n' | sudo tee -a /etc/pacman.conf",
      "pacman -Syyu visual-studio-code-bin --noconfirm helium-browser-bin fresh-editor localsend paru postman-bin spotify ttf-ms-fonts",
    ],
